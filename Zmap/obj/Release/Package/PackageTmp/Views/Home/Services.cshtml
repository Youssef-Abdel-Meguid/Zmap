@model Zmap.Dtos.ServicesDto
@{
    ViewBag.Title = "Services";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="inn-body-section pad-bot-50">
    <div class="container">
        <div class="row inn-page-com">
            <div class="page-head">
                <h2>@Model.Title</h2>
                <div class="head-title">
                    <div class="hl-1"></div>
                    <div class="hl-2"></div>
                    <div class="hl-3"></div>
                </div>
                <p>@Model.Details</p>
            </div>
            <!--SERVICES SECTION-->
            <div class="col-md-12">
                @foreach (var item in Model.Services)
                 {
                    <div class="row inn-services in-blog">
                        <div class="col-md-4"> <img src="~/Images/Admin/Services/@item.PhotoUrl" alt="" /> </div>
                        <div class="col-md-8">
                            <h3>@item.Title</h3>
                            <p>@item.Details</p>
                        </div>
                    </div>
                 }

            </div>
            <!--END SERVICES SECTION-->
        </div>
    </div>
</div>


