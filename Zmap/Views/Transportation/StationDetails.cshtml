@model Zmap.Dtos.StationDto

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    <div class="inn-body-section pad-bot-55">
        <div class="page-head">
            <h2>ادارة @Model.Name</h2>
            <div class="head-title">
                <div class="hl-1"></div>
                <div class="hl-2"></div>
                <div class="hl-3"></div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="col-md-12">
                <div class="hp-amini hp-amini-block db-div-card">
                    <ul>
                       
                        <li class="db-card">
                            <a href="@Url.Action("Contacts", "Transportation", new { Id = Model.Id, isCompany = false })">
                                <img src="~/Images/icon/addson.png" class="db-card-img" />
                                <p>ارقام المحطة</p>
                            </a>
                        </li>

                        <li class="db-card">
                            <a href="@Url.Action("Gallery", "Transportation", new { Id = Model.Id, flag = 2 })">
                                <img src="~/Images/icon/gallery.png" class="db-card-img" />
                                <p>صور المحطة</p>
                            </a>
                        </li>

                    </ul>
                </div>
            </div>


            <a href="@Url.Action("Details", "Transportation", new { id = Model.CompanyId })" class="btn-floating btn-large waves-effect waves-light blue">
                <i class="material-icons">arrow_back</i>
            </a>
        </div>
    </div>

</div>